{
  "env": {
    "node": true,
    "es2021": true,
    "es6": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "off",
    "space-before-function-paren": [
      "error",
      "always"
    ],
    "no-console": 1,
    "no-empty": 1,
    "no-irregular-whitespace": 1,
    "no-useless-catch": 0,
    "linebreak-style": 0,
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1
      }
    ],
    "indent": [
      "error",
      "tab"
    ],
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ],
    "semi": [
      "error",
      "never"
    ]
  }
}